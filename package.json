{
  "name": "@solx/css-units",
  "version": "0-0-0-development",
  "description": "Array of All CSS Media Units. ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "ts-node src/index.ts",
